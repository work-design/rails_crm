<%= render 'contact_bar' %>

<div class="cards is-4">
  <div class="card">
    <div class="card-content text-centered">
      <strong><%= @contact.orders.count %></strong>
      <p>Orders Count</p>
    </div>
  </div>
  <div class="card">
    <div class="card-content text-centered">
      <strong><%= @contact.pending_members.count %></strong>
      <p>Contacts Count</p>
    </div>
  </div>
</div>

<dl class="as-show is-boxed">
  <%= render 'show_table' %>
</dl>
