<div class="ui modal" id="modal">
  <i class="close icon"></i>
  <div class="ui header blue">
    <%= t('.edit') %>
  </div>

  <div class="content">
    <%= form_with url: order_admin_maintain_path(@maintain), method: :patch do |f| %>
      <%= f.collection_select :card_template_id, @card_templates, :id, :name, label: '选择卡' %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
