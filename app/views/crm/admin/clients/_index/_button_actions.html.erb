<%= form_with url: { action: 'new' }, class: 'button_to' do |f| %>
  <%= f.hidden_field :ids, data: { form_target: 'ids' } %>
  <%= button_tag data: { form_target: 'submit' }, aria: { label: t('.assign') }, disabled: true, class: 'button is-small is-dark' do %>
    <i class="fa-solid fa-hand-holding-usd"></i>
  <% end %>
<% end %>
