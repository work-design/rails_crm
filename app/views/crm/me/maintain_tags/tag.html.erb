<div class="weui-panel">
  <div class="weui-panel__bd">
    <span><%= @tag.name %></span>
    <div class="image">
    <% if @primary_material %>
      <%= image_tag @primary_material.url_with_watermark(@contact.file) %>
    <% else %>
      <%= image_tag @contact.qr_code %>
    <% end %>
    </div>
  </div>
</div>
