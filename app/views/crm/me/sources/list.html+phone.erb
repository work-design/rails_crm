<%= render 'top_bar' %>

<div class="box mx-3">
<% @sources.each do |source| %>
  <%= link_to({ action: 'source', source_id: source.id }, state: 'enter', class: 'box-cell') do %>
    <span class="box-cell-body"><%= source.name %></span>
    <span class="weui-cell__ft"></span>
  <% end %>
<% end %>
</div>
